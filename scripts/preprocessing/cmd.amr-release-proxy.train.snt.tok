#!/bin/bash

cat "${DATA_DIR}/amr-release-proxy.train" | grep '::snt ' | sed 's/^# ::snt //' | "${CDEC}/corpus/tokenize-anything.sh" > "${DATA_DIR}/amr-release-proxy.train.snt.tok"
cat "${DATA_DIR}/amr-release-proxy.dev" | grep '::snt ' | sed 's/^# ::snt //' | "${CDEC}/corpus/tokenize-anything.sh" > "${DATA_DIR}/amr-release-proxy.dev.snt.tok"
cat "${DATA_DIR}/amr-release-proxy.test" | grep '::snt ' | sed 's/^# ::snt //' | "${CDEC}/corpus/tokenize-anything.sh" > "${DATA_DIR}/amr-release-proxy.test.snt.tok"
