#!/bin/bash

for split in train dev test; do
    java -mx500m -cp ".:${STANFORD_PARSER}" edu.stanford.nlp.parser.lexparser.LexicalizedParser -sentences newline -outputFormat "penn" grammar/englishPCFG.ser.gz "${DATA_DIR}/amr-release-proxy.dev.snt" > "${DATA_DIR}/amr-release-proxy.${split}.snt.tree" &
done
